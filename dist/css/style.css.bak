/*==================================================*
 *
 *  OgML CSS Start
 *
 *==================================================*/

body{
    position: relative;
    width: 100%;
    height: 100%;
    background: #f0f0f0;
}

#container{
    position: absolute;
    width: 960px;
    height: 540px;
    left: 50%;
    /* top: 50%; */
    /* margin-left: -480px; */
    /* margin-top: -270px; */
	margin-left: -480px;
    margin-top:20px;
    background: #333;
    box-shadow: 0 0 2px rgba(0,0,0,.5);
}

#wrapper{
    position: absolute;
    width: 100%;
    height: 100%;
    /* background: #fff url(../img/bga.jpg) no-repeat; */
}

#container:after{
    display: block;
    content: "";
    clear: both;
}

#bg, #fp, #ca{
    position: absolute;
    width: 100%;
    height: 100%;
    margin: 0;
}

#bg, #fg, #ca{
    text-align: center;
}

#bg-img img, #fg-img img{
    position: absolute;
    width: 100%;
    height: 100%;
    left: 0;
}

#ctrl{
    /* width: 100%; */
    position: absolute;
    left: 1px;
    right: 1px;
    bottom: 0;
    height: 115px; /* 162px - 20px */
    background-color: rgba(219,139,0,.1);
	background-image: url(../img/argo_wordbg.png);
    font-size: 1rem;
    line-height: 1.6;
    color: #fff;
    padding: 10px;
}


#ctrl-bar{
	position:absolute;
	right:20px;	
	top:5px;
}

#save,#load,#menu,#help{
	display:inline-block;
	padding:0px 12px 2px 12px;
	line-height:1.2em;
	font-family:'century_gothicregular',Helvetica,Arial;	
	font-size:1.2em;
	font-weight:100;
	color:rgba(230,230,230,0.8);
	letter-spacing:0.08em;
	cursor: pointer;
}

#save:hover,#load:hover,#menu:hover,#help:hover{
	color:rgba(255,255,255,1);
}

#talker{
    position: absolute;
    bottom: 145px;
    padding: 4px 10px;
    left: 250px;
    background: rgba(0,0,0,.6);
    font-size: 1.2rem;
    border-radius: 5px;
    text-align: center;
    min-width: 4em;
    line-height: 1.8;
    height: 1.8em;
	font-family:"微软雅黑","黑体","宋体";
}

#hidden{
    height: 0;
    width: 0;
    overflow: 0;
    visibility: hidden;
}

#select-outer{
	font-family:'黑体',Helvetica,Arial,"微软雅黑";
	position:absolute;
	left:0px;
	width:100%;
	height:540px;
	bottom:0px;
	margin:0px;
	padding:0px;
	border:0px;
	background-color:rgba(0,0,0,.7);
}

#select{
	font-size:1.1em;
	white-space:pre-wrap;
	bottom:0px;
	margin-left:auto;
	margin-right:auto;
	position:relative;
	width:400px;
	color:#F0F0F0;
	letter-spacing:0.08em;
	height:auto;
	margin-bottom:165px;
}

#select > span{
	height:2.5em;
	padding:2em;
}

#select > div{
	width:100%;
	/* text-indent:-1.6em; */
	text-align:left;
    border: 1px solid transparent;
    padding: 0.2em 0.4em 0.2em 2.8em;
	cursor: pointer;
}

#select > div:first-letter{
	margin-left:-2em;
}

#select > div:hover{
    /* border-color: #444; */
	cursor: pointer;
	background:rgba(120,120,120,0.6);
	color:#FFFFFF;
	/* width:100%; */
}

#words{
	/* left:250px; */
	font-family:"宋体",Helvetica,Arial,"微软雅黑";
	font-size:1.2em;
	margin-top:20px;
	float:left;
	position:relative;
	display:inline-block;
	vertical-align:top;
	margin-left:50px; 
	margin-right:auto;
}

#avatar{
	float:left;
	position:relative;
	height:100%;
	width:250px;
	margin-left:10px;
	/* display:inline-block; */
}

#avatar-inner{
	position: absolute;
	bottom: -10px; /* touch the game screen bottom */
	width: 300px;
	height: auto;
	left:-35px;
	/* overflow:hidden; */
	/* z-index: 1;	 */
	margin:0px;
		/* margin-bottom:-10px; */
}


#nametag{
	position:absolute;
	right:-180px;
	top:-75px;
}

#home,#home-bg{
	position:absolute;
	top:0px;
	left:0px;
	hegiht:100%;
	width:100%;
}

#home-title{
	display:none; /*Not used in this Argo X'mas project*/
	position:absolute;
	top:50px;
	left:50px;
	height:89px;
	width:373px;
	background:url('../resources/home-title.png');
}

#home-start{
	position:absolute;
	cursor:pointer;
	top:150px;
	left:100px;
	height:34px;
	width:134px;
	background:url('../resources/home-start.png');
}

#home-continue{
	position:absolute;
	top:200px;
	cursor:pointer;
	left:150px;
	height:33px;
	width:134px;
	background:url('../resources/home-continue.png');
}

#home-argo{
	position:absolute;
	cursor:pointer;
	top:250px;
	left:200px;
	height:30px;
	width:134px;
	background:url('../resources/home-argo.png');
}